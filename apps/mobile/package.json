    "react-native-logs": "^5.5.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-unistyles": "^3.0.0-alpha.44",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
