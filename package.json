{
  "name": "matrapp",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:ios": "turbo run dev:ios",
    "dev:android": "turbo run dev:android",
    "check": "turbo run check",
    "check:fix": "turbo run check:fix",
    "check-types": "turbo run check-types",
    "codegen": "dotenv -- turbo run codegen --ui=stream",
    "graph:up": "turbo run docker:up --filter=@matrapp/subgraph-*",
    "graph:down": "turbo run docker:down --filter=@matrapp/subgraph-*",
    "graph:build": "turbo run build --filter=@matrapp/subgraph-*",
    "graph:test": "turbo run test --filter=@matrapp/subgraph-*"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/bun": "^1.3.6",
    "dotenv-cli": "^11.0.0",
    "turbo": "^2.7.5",
    "typescript": "5.9.2"
  },
  "packageManager": "bun@1.3.6",
  "workspaces": [
    "apps/*",
    "apps/subgraphs/*",
    "packages/*"
  ],
  "dependencies": {
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/client-preset": "^5.2.2",
    "@uniswap/sdk-core": "^7.10.1",
    "@uniswap/v3-sdk": "^3.27.0",
    "graphql-request": "^7.4.0",
    "numbro": "^2.5.0",
    "viem": "^2.44.4"
  },
  "overrides": {
    "jsbi": "3.2.5"
  }
}
